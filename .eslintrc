{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["plugin:@typescript-eslint/recommended"],
  "rules": {
    "no-console": "warn",
    "quotes": ["warn", "single"],
    "semi": "error",
    "indent": ["error", 2],
    "max-len": [
      "error",
      {
        "code": 120
      }
    ]
  }
}
